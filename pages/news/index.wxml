<!--pages/news/index.wxml-->
<view class='container'>
  <my-tabs nav="{{nav}}" bind:tabNav="tabNav" bind:edit="edit"></my-tabs>
  <view class='list'>
    <block>
      <i-swipeout  i-class="i-swipeout-demo-item" actions="{{actions}}">
        <view slot="content" class='flex'>
          <view class='img-box'>
            <image src='../../static/img/user.png'></image>
            <view class='no-flag'></view>
          </view>
          <view class="i-swipeout-des">
            <view class="i-swipeout-des-h2 flex-between">
              <view class='name'>第七个小矮人</view>
              <view class='time'>11:26</view>
            </view>
            <view class="i-swipeout-des-detail l-o">乐观善良的7个小矮人原本过着简单快乐的生活，不料诅咒公主的巫婆利用小矮人进入.</view>
          </view>
        </view>
      </i-swipeout>
    </block>
  </view>
</view>
