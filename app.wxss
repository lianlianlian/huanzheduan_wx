/**app.wxss**/
@import "utils/wxToast/wetoast.wxss";
page{
  background: #f3f3f3;
  color: #060606;
  font-size: 14px;
  line-height: 1.6;
  font-family: 'PingFang SC', 'Hiragino Sans GB', 'Helvetica Neue', 'Microsoft Yahei', 'WenQuanYi Micro Hei', sans-serif;
}
view,image,button,scroll-view,page,textarea,video{
  box-sizing: border-box;
}
video{
  width: 100%;
}
button::after{
  border: none;
}
image{
  vertical-align: middle;
}
.toast{
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 400rpx;
  line-height: 80rpx;
  border-radius: 5px;
  color: #fff;
  background: rgba(0, 0, 0, 0.8);
  font-size: 15px;
  text-align: center;
  z-index: 1000;
}
.clear::after{
  content: "";
  display: block;
  overflow: hidden;
  height: 0;
  clear: both;
}
.group{
  margin-top: 20rpx;
}
.cell{
  display: flex;
  align-items: center;
  position: relative;
  padding: 35rpx 30rpx;
  background: #fff;
}
.cell:before,.block-cell::before{
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  border-bottom: 1px solid #f0f0f0;
}
.cell .title{
  font-size: 15px;
  color: #272727;
}
.cell .right{
  flex: 1;
}
.block-cell{
  position: relative;
  background: #fff;
  padding: 25rpx 30rpx 30rpx;
}
.block-title{
  margin-bottom: 20rpx;
  font-size: 16px;
}
.border-bottom,.border-top{
  position: relative;
}
.border-top:before{
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  border-top: 1px solid #f0f0f0;
}
.border-bottom:before{
  content: "";
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  border-bottom: 1px solid #f0f0f0;
}
.flex{
  display: flex;
  align-items: center;
}
.flex-around{
  display: flex;
  align-items: center;
  justify-content: space-around;
}
.flex-between{
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.is-link{
  position: relative;
}
.is-link:after{
  content:" ";
  display:inline-block;
  height:6px;
  width:6px;
  border-width:2px 2px 0 0;
  border-color:#c8c8cd;
  border-style:solid;
  -webkit-transform:matrix(.71,.71,-.71,.71,0,0);
  transform:matrix(.71,.71,-.71,.71,0,0);
  top:-2px;
  position:absolute;
  top:50%;
  margin-top:-4px;
  right:40rpx;
}
/*加减*/
.x-inline-number{
  display: flex;
}
.x-inline-number .btn{
  width: 60rpx;
  height: 60rpx;
  line-height: 60rpx;
  border: 1px solid #f0f0f0;
  text-align: center;
  font-size: 11px;
}
.x-inline-number .input{
  width: 100rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-top: 1px solid #f0f0f0;
  border-bottom: 1px solid #f0f0f0;
  text-align: center;
}
.padding-0{
  padding: 0 !important;
}
/*底部操作*/
.fixed{
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 20rpx 10rpx;
  background: #fff;
  display: flex;
  align-items: center;
}


/*tab*/
.tab-nav{
  display: flex;
  align-items: center;
  justify-content:  space-around;
  background: #fff;
  height: 100rpx;
}
.tab-nav .item{
  /* border-bottom: 1px solid transparent; */
  line-height: 100rpx;
}
/* .tab-nav .item.active{
  border-bottom: 1px solid #363636;
} */
/* 隐藏滚动条 */
::-webkit-scrollbar {
  width: 0; 
  height: 0;
  display: none;
  color: transparent;
}

/*loadmore*/
.weui-loadmore{
  width:65%;
  margin:15px auto;
  line-height:1.6em;
  font-size:14px;
  text-align:center;
}
.weui-loading {
  margin: 0 5px;
  width:20px;
  height:20px;
  display: inline-block;
  vertical-align: middle;
  animation: weuiLoading 1s steps(12, end) infinite;
  background: transparent url(data:image/svg+xml;base64,) no-repeat;
  background-size: 100%;
  
}

@keyframes weuiLoading {
  0% {
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    transform: rotate3d(0, 0, 1, 360deg);
  }
}
.weui-loadmore__tips{
  display:inline-block;
  vertical-align:middle;
}
.weui-loadmore_line{
  border-top:1px solid #e5e5e5;
  margin-top:2.4em;
}
.weui-loadmore__tips_in-line{
  position:relative;
  top:-.9em;
  padding:0 .55em;
  background-color:#f4f4f4;
  color:#999;
}


/*searchbar*/
/*@weuiSearchBarHeight: 28px;*/
.weui-search-bar {
    position: relative;
    padding: 8px 10px;
    display: flex;
    box-sizing: border-box;
    background-color: #EFEFF4;
    border-top: 1rpx solid #D7D6DC;
    border-bottom: 1rpx solid #D7D6DC;
}
.weui-icon-search {
    margin-right: 8px;
    font-size:inherit;
}
.weui-icon-search_in-box {
    position: absolute;
    left: 10px;
    top: 7px;
}
.weui-search-bar__text{
    display: inline-block;
    font-size: 14px;
    vertical-align: middle;
}
.weui-search-bar__form {
    position: relative;
    flex: auto;
    border-radius: 5px;
    background: #FFFFFF;
    border: 1rpx solid #E6E6EA;
}
.weui-search-bar__box {
    position: relative;
    padding-left: 30px;
    padding-right: 30px;
    width: 100%;
    box-sizing: border-box;
    z-index: 1;
}
.weui-search-bar__input {
    height: 28px;
    line-height: 28px;
    font-size: 14px;
}
.weui-icon-clear {
    position: absolute;
    top: 0;
    right: 0;
    padding: 7px 8px;
    font-size: 0;
}
.weui-search-bar__label {
    position: absolute;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 2;
    border-radius: 3px;
    text-align: center;
    color: #9B9B9B;
    background: #FFFFFF;
    line-height: 28px;
}
.weui-search-bar__cancel-btn {
    margin-left: 10px;
    line-height: 28px;
    color: #09BB07;
    white-space: nowrap;
}
.container{
  min-height: 100%;
}
.l-o{
  display: -webkit-box;
  word-break: break-all;
  overflow: hidden; 
  text-overflow: ellipsis;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.d-o{
  display: -webkit-box;
  line-height: 60rpx;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
  text-overflow:ellipsis;
}
.browse-box .browse{
  font-size: 13px;
}
.browse-box .time{
  margin-left: 10rpx;
  font-size: 12px;
  color: #adadad;
}
.browse-box .ear-img{
  width: 40rpx;
  height: 26rpx;
  margin-right: 10rpx;
}
.common-img{
  width: 30rpx;
  height: 26rpx;
  margin-right: 10rpx;
}
.mark{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.3);
}
.flag{
  margin-left: 10rpx;
  border-radius: 35rpx;
  background: #ffe056;
  font-size: 13px;
  padding: 3rpx 10rpx;
  font-weight: normal;
}
.submit-box {
  padding: 60rpx;
}
.submit-btn{
  background: #86858c;
  font-size: 17px;
  color: #fff;
}
.submit-btn.red {
  background: #ff4444;
}
.placeholder{
  color: #adadad;
}
.textarea-box {
  padding: 30rpx 35rpx;
  background: #fff;
  color: #272727;
  font-size: 15px;
}
.textarea-box textarea{
  padding: 15rpx 25rpx;
  background: #f3f3f3;
  width: 100%;
  height: 405rpx;
  border-radius: 5rpx;
}
.hospital{
  padding: 20rpx 35rpx;
  font-weight: bold;
  font-size: 16px;
}
.hospital .distance{
  font-size: 15px;
  font-weight: normal;
  color: #828282;
}
.hospital-infor{
  padding: 45rpx 35rpx;
  background: #f3f3f3;
}
.hospital-addr,.hospital-time{
  margin-bottom: 25rpx;
  display: flex;
  font-size: 15px;
}
.hospital-infor image {
  width: 34rpx;
  height: 41rpx;
  margin-right: 10rpx;
}

.msg-box {
  padding: 0 30rpx 30rpx;
  background: #fff;
}
.msg-box .msg-list{
  margin: 0 -30rpx;
}
.msg-title {
  padding: 25rpx 0;
}
.msg-title .name{
  font-weight: bold;
  font-size: 18px;
}
.msg-title .more{
  padding-right: 25rpx;
  color: #adadad;
  font-size: 13px;
}
.msg-title.is-link:after{
  right: 0;
}
.collect-img{
  width: 38rpx;
  height: 37rpx;
}
.order-title{
  position: relative;
  padding: 25rpx 0;
  font-weight: bold;
}
.order-title::after{
  content: "";
  position: absolute;
  left: -35rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 15rpx;
  height: 30rpx;
  background: #ff4445;
}